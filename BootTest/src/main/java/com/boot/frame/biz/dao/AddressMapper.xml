<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.boot.frame.biz.dao.AddressMapper">



    <select id="find" resultType="java.lang.String" parameterType="com.boot.frame.biz.entity.AddressRequest">
            SELECT
                fname
            FROM
                t_address
            WHERE
                state = 1
            AND id = #{id}
    </select>



</mapper>